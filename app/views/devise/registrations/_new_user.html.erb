<%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: "needs-validation", :novalidate => true }) do |f| %>
  <%= devise_error_messages! %>
    <div class="col-12" style="padding: 0px;">
      <label class="label">Nombre Completo</label>
      <%= f.text_field :names, autofocus: true, autocomplete: "email", class: "form1 form-control", id: "validationCustom01", :required => true %>

      <label class="label">Correo Electronico</label>
      <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form1 form-control", id: "validationCustom01", :required => true %>

      <div class="invalid-feedback">
            Requerido!
      </div>  
      
    </div>

    <div class="form-row">

      <div class="f16 col-md-6">
        <label class="label">Pais</label>
          <input type="text" id="country" class="form1 form-control" placeholder="" name="user[city]"  />
      </div>

      <div class="col-md-6">
        <label class="label">Numero de Celular</label>
        <%= f.text_field :phone, autofocus: true, autocomplete: "email", class: "form1 form-control", id: "validationCustom01", :required => true %>
          <div class="invalid-feedback">
            Requerido!
          </div>        
      </div>
                                
    </div>

    <div class="form-row">

      <div class="col-md-6">
        <label class="label">Contraseña</label>
        <%= f.password_field :password, autocomplete: "new-password", class: "form1 form-control", id: "validationCustom01", :required => true  %>
          <div class="invalid-feedback">
            Requerido!
          </div>   
      </div>

      <div class="col-md-6">
        <label class="label">Confirmar Contraseña</label>
        <%= f.password_field :password_confirmation, autocomplete: "new-password", class: "form1 form-control", id: "validationCustom01", :required => true  %>   
          <div class="invalid-feedback">
            Requerido!
          </div>                              
      </div>

    </div>




<script src="https://cdn.jsdelivr.net/npm/places.js@1.16.1"></script>
<script>
(function() {
  var placesAutocomplete = places({
    appId: 'plCC6QC1KFHV',
    apiKey: '1c3b9226a5891eafe44c0f90feff3b56',
    container: document.querySelector('#country'),
    templates: {
      suggestion: function(suggestion) {
        return '<i class="flag ' + suggestion.countryCode + '"></i> ' +
          suggestion.highlight.name;
      }
    }
  }).configure({
    type: 'country',
  });
})();
</script>

  <div class="col-12 text-center">
    <br>
    <%= f.submit "Crear Cuenta", class: "btn1" %>
  </div>
<% end %>

<style type="text/css" media="screen">
  .modal-open{
  padding: 0px !important;
}

.ap-dropdown-menu{
  color: #000000a8 !important;
}
</style>