<% if devise_error_messages!.present? %>
	toastr["error"]("Error <br> <b><%= flash[:error] = resource.errors.full_messages.first %></b>");
<% else %>
	toastr["success"]("Tu Informacion se Actulizo con <b> Exito!</b>");
	<% if @user.avatar.present? %>
		document.getElementById("user-avatar1").src = '<%= @user.avatar.url() %>';
	<% end %>

<% end %>
