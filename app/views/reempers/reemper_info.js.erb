$("#card-users").fadeIn("show");
$("#name").text("<%= @reemper.user.names %>");
$("#cirugia").text("<%= @reemper.profession %>");
$("#biograpy").text("<%= @reemper.biography %>");

<% @valoration.each do |valoracion| %>
	$("#valoration").append('<%= j render "parciales/user", valoracion: valoracion %>');
<% end %>

document.getElementById("reemper-img").src = '<%= @reemper.user.avatar.url() %>';